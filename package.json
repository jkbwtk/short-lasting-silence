{
  "name": "short-lasting-silence",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun ./tools/devServer",
    "build": "bun run build:client && bun run build:server",
    "build:client": "cross-env NODE_ENV=production bun run --bun vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env NODE_ENV=production bun run --bun vite build --ssr src/entryServer.tsx --outDir dist/server",
    "prerender": "bun run build && cross-env NODE_ENV=production bun ./tools/prerenderPages.ts",
    "preview": "bun run prerender && cross-env NODE_ENV=production bun ./tools/staticServer",
    "lint": "bun run --bun biome check --write",
    "test": "bun run --bun vitest",
    "visualize": "bun run --bun vite-bundle-visualizer -o bundle.html",
    "style-types": "bun run --bun typed-scss-modules src/styles",
    "fetch-symbols": "bun run ./tools/fetchMaterialSymbols"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.10",
    "pure-rand": "^6.1.0",
    "solid-js": "^1.9.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.6.2",
    "@types/bun": "^1.1.12",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@vitest/coverage-v8": "^2.1.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.21.1",
    "jsdom": "^25.0.1",
    "sass-embedded": "^1.80.4",
    "sirv": "^2.0.4",
    "terser": "^5.36.0",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.1.3"
  },
  "packageManager": "pnpm@9.4.0+sha1.9217c800d4ab947a7aee520242a7b70d64fc7638"
}
