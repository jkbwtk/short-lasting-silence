{
  "name": "short-lasting-silence",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun ./tools/devServer",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entryServer.tsx --outDir dist/server",
    "preview": "pnpm build && bun ./tools/prerenderPages.ts && cross-env NODE_ENV=production bun ./tools/staticServer",
    "lint": "biome lint --write && biome format --write"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.7",
    "@types/bun": "^1.1.10",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "pure-rand": "^6.1.0",
    "sirv": "^2.0.4",
    "solid-js": "^1.9.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "cross-env": "^7.0.3",
    "sass-embedded": "^1.79.4",
    "terser": "^5.34.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-dts": "^4.2.3",
    "vite-plugin-solid": "^2.10.2"
  },
  "packageManager": "pnpm@9.4.0+sha1.9217c800d4ab947a7aee520242a7b70d64fc7638"
